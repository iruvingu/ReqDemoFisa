<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="horizontal"
        android:weightSum="12"
        xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
            android:id="@+id/snameColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="center"
            android:background="@color/rowPar"
            android:text="Sucursal"
            android:layout_margin="1dp"
            android:textSize="10sp"
            android:textColor="@color/black"/>

    <TextView
            android:id="@+id/sfirstColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="EPRC"
            android:layout_margin="1dp"
            android:textSize="10sp"/>

    <TextView
            android:id="@+id/ssecondColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="0-14"
            android:layout_margin="1dp"
            android:textSize="10sp"/>
    <TextView
            android:id="@+id/sthirdColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="30-59"
            android:layout_margin="1dp"
            android:textSize="10sp"/>
    <TextView
            android:id="@+id/sfourthColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="90+"
            android:layout_margin="1dp"
            android:textSize="10sp"/>

    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="center"
            android:layout_margin="1dp"
            android:textSize="10sp"/>

    <TextView
            android:id="@+id/sfifthColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="EPRC"
            android:layout_margin="1dp"
            android:textSize="10sp"/>
    <TextView
            android:id="@+id/ssixthColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="0-14"
            android:layout_margin="1dp"
            android:textSize="10sp"/>
    <TextView
            android:id="@+id/sseventhColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="30-59"
            android:layout_margin="1dp"
            android:textSize="10sp"/>
    <TextView
            android:id="@+id/seightColumn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/rowInpar"
            android:text="90+"
            android:layout_margin="1dp"
            android:textSize="10sp"/>

</LinearLayout>