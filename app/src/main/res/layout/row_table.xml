<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

    <TextView
            android:id="@+id/row_table_name"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:text="Sucursal"
            android:textSize="10sp"
            android:layout_margin="1dp"
            android:background="@color/rowInpar"
            android:gravity="center"
    />

    <TextView
            android:id="@+id/row_table_actual"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:text="40%"
            android:layout_gravity="center"
            android:textSize="10sp"
            android:layout_margin="1dp"
            android:background="@color/rowInpar"
            android:gravity="center"
    />

    <TextView
            android:id="@+id/row_table_proyection"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:text=" 40%"
            android:layout_gravity="center"
            android:textSize="10sp"
            android:layout_margin="1dp"
            android:background="@color/rowInpar"
            android:gravity="center"
    />

    <TextView
            android:id="@+id/row_table_meta"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:text="40%"
            android:layout_gravity="center"
            android:textSize="10sp"
            android:layout_margin="1dp"
            android:background="@color/rowInpar"
            android:gravity="center"
    />

    <TextView
            android:id="@+id/row_table_rank"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:text="Rank"
            android:layout_gravity="center"
            android:textSize="10sp"
            android:layout_margin="1dp"
            android:background="@color/rowInpar"
            android:gravity="center"
    />

    <TextView
            android:id="@+id/row_table_taso"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:text="Taso"
            android:layout_gravity="center"
            android:textSize="10sp"
            android:layout_margin="1dp"
            android:background="@color/rowInpar"
            android:gravity="center"
    />

</LinearLayout>